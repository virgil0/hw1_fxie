<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>GeneTypeSystem</name>
  <description>Type System Definition for gene mention tagging</description>
  <version>1.0</version>
  <vendor>The Apache Software Foundation</vendor>
  <types>
    <typeDescription>
      <name>org.apache.uima.genetypesystem.id</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>id</name>
          <description>id of sentence</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>org.apache.uima.genetypesystem.genetag</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>genetag</name>
          <description>identified gene name</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
